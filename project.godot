config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false
resources_spreadsheet_view/fold_docks=false

[application]

config/name="Turn-Based Combat"
config/description="[Open Source] Godot 4 Turn Based Battle System made for the game series: \"Cute Fame\", Avaliable for use freely with given credit to the project. (Which may change in the future)

Repository: https://github.com/Cute-Fame-Studio/3D-TurnBasedCombat"
config/version="1.0"
run/main_scene="res://maps/battle_scenes/test.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GlobalBattleSettings="*res://assets/globals/battle-settings/GlobalBattleSettings.gd"
SaveLoader="*res://assets/globals/save-system/save_loader.gd"
AIManager="*res://battle-manager/ai_manager.gd"
SignalBus="*res://signal_bus.gd"
ItemHandler="*res://battle-manager/inventory/item_handler.gd"

[dotnet]

project/assembly_name="Turn-Based Combat"

[file_customization]

folder_colors={
"res://animations/": "red",
"res://assets/": "orange",
"res://battle-manager/": "teal",
"res://database/": "blue"
}

